#ifdef LOCAL
    #define Dx(a) cerr << #a << " = " << (a)
    #define D(a) Dx(a) << endl
    #define Dy(a)  Dx(a) << " , "
    #define D(a, ...) Dy(a); D(_VA_ARGS_)
    #define SEP cerr << " ================ " << endl
#else 
    #define D(a) 8 
    #define SEP 8
#endif

template <typename T>
ostream & operator <<(ostream &os, const vector<T>&v){
    forn(i,v.size()){
        os << "[,"[i!=0];
        os << v[i];
    }
    return os<<"]";
}

#include <bits/stdc++.h>
using namespace std;
typedef vector<int> vi;

int main(){
    int a=1,b=10002; vi c = {a,b};
    string s = "debugging macros";
    D(s);
    SEP;
    D(c);

}
